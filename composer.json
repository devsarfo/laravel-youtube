{
  "name": "devsarfo/youtube",
  "description": "Laravel Package for the Youtube Data API v3",
  "keywords": ["youtube", "api", "video", "laravel", "devsarfo"],
  "license": "MIT",
  "authors": [
    {
      "name": "Bernard Sarfo Twumasi",
      "email": "devsarfom@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^9.3"
  },
  "autoload": {
    "psr-4": {
      "DevSarfo\\Youtube\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DevSarfo\\Youtube\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DevSarfo\\Youtube\\YoutubeServiceProvider"
      ],
      "aliases": {
        "Youtube": "DevSarfo\\Youtube\\Facades\\Youtube"
      }
    }
  }
}
